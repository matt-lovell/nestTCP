{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-typescript-starter/src",
  "monorepo": true,
  "root": "apps/nest-typescript-starter",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest-typescript-starter/tsconfig.app.json"
  },
  "projects": {
    "nest-typescript-starter": {
      "type": "application",
      "root": "apps/nest-typescript-starter",
      "entryFile": "main",
      "sourceRoot": "apps/nest-typescript-starter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-typescript-starter/tsconfig.app.json"
      }
    },
    "hello-microservice": {
      "type": "application",
      "root": "apps/hello-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/hello-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hello-microservice/tsconfig.app.json"
      }
    }
  }
}